{
    "name": "jsdoc",
    "main": "src/index.js",
    "scripts": {
        "build": "tsc",
        "lint": "eslint src",
        "lint:report": "eslint src -f json -o report.json --no-fix",
        "lint:fix": "eslint src --fix",
        "test": "c8 node --test src/index.test.js",
        "test:watch": "c8 node --watch --test src/index.test.js"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "c8": "^9.0.0",
        "eslint": "^8.0.0",
        "eslint-plugin-jsdoc": "^48.0.0",
        "typescript": "^5.0.0"
    },
    "type": "module",
    "engines": {
        "node": ">= 20"
    }
}